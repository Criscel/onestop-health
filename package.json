{
  "name": "onestop-health",
  "version": "1.0.0",
  "description": "project3",
  "main": "server.js",
  "engines": {
    "node": "14.15.x"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Criscel/onestop-health.git"
  },
  "author": "Criscel Agdeppa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Criscel/onestop-health/issues"
  },
  "homepage": "https://github.com/Criscel/onestop-health#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-openid-connect": "^2.4.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.8",
    "node-stream": "^1.7.0",
    "react-icons": "^4.2.0",
    "serve": "^11.3.2",
    "stream": "0.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
